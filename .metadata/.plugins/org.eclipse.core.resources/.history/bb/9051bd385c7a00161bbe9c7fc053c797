package com.cmbchina.di;

public class Student {

	private String name;
	private Address addr;
	
	public void setName(String name) {
		this.name = name;
	}
	
	public void setAddress(Address address) {
		this.addr = address;
	}
	
	public void show(){
		System.out.println("name="+name);
		System.out.println("addr="+addr.getAddress());
	}
}
