package com.cmbchina.log;

import java.lang.reflect.Method;

import org.springframework.aop.MethodBeforeAdvice;

public class Log{
	/**
	 * @param method 指被调用的方法对象
	 * @param args 被调用的方法的参数
	 * @param target 被调用的方法的目标对象
	 */
	@Override
	public void before(){
		// TODO 自动生成的方法存根
		System.out.println("------方法执行前------");
	}
	
	public void after(){
		// TODO 自动生成的方法存根
		System.out.println("------方法执行前------");
	}
	
}
