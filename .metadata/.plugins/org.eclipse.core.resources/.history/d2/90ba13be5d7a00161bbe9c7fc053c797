package com.cmbchina.di;

public class Student {

	public String name;
	public Address addr;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setAddress(Address address) {
		this.addr = address;
	}
	
	public void show(){
		System.out.println("Name:"+name);
		System.out.println("Addr:"+addr.getAddress());
	}
}
